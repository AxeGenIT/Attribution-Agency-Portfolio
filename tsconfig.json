{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "paths": {
    "@/*": ["./src/*"]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/helpers/backend.js"
, "app/(frontend)/(pages)/auth/_components/SignUp.tsx", "app/(frontend)/(pages)/auth/page.tsx", "app/(frontend)/(pages)/auth/_components/SignIn.tsx", "providers/Provider.jsx", "context/ProviderContext.js", "app/(frontend)/(pages)/demo/_components/TimePicker.js", "app/(frontend)/(pages)/sample-report/page.js"  ],
  "exclude": ["node_modules"]
}
